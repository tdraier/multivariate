<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="org.jahia.modules.multivariate.MultivariateFilter">
        <property name="renderService" ref="RenderService"/>
        <property name="applyOnConfigurations" value="module" />
        <property name="applyOnNodeTypes" value="jnt:multivariate" />
        <property name="applyOnModes" value="live,preview" />
        <property name="priority" value="20" />
    </bean>

</beans>